{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["es2021", "DOM"],
    "jsx": "react",
    "module": "CommonJS",
    "rootDir": "src",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowJs": true,
    "outDir": "build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  }
}
